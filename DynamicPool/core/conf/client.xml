<server>
	<processors>
		<command name="synchoronous">vn.edu.hust.student.dynamicpool.dal.processor.SynchoronousProcessor</command>
		<command name="sendSettings">vn.edu.hust.student.dynamicpool.dal.processor.SendSettingsProcessor</command>
	</processors>
	
	<http>
		<network>
			<entry>
				<path>/*</path>
				<port>6969</port>
			</entry>
		</network>
		<handlers>
			
		</handlers>
	</http>
	
	<socket>
		<network>
			<entry>
				<port>9696</port>
			</entry>
		</network>
		<handlers>
		  	<handler>sendSettings</handler>
		</handlers>
	</socket>
	
</server>
